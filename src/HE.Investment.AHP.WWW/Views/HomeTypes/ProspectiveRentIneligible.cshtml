@using HE.Investment.AHP.WWW.Extensions
@using HE.Investment.AHP.WWW.Views.Shared.Components.HomeTypeFormHeader
@using HE.Investment.AHP.Contract.HomeTypes
@using HE.Investments.Common.WWW.Extensions
@model HE.Investment.AHP.WWW.Models.HomeTypes.HomeTypeBasicModel;
@{
    var applicationId = this.GetApplicationIdFromRoute();
    var homeTypeId = this.GetHomeTypeIdFromRoute();
}

<gds-div-grid-column-two-thirds>
    <vc:home-type-form-header current-page="@HomeTypesWorkflowState.ProspectiveRentIneligible"
                              caption="@Model.Header"
                              title="Contact your Growth Manager"/>
    <gds-p>
        Funding is not available for homes with greater than 3% of the proposed rent as a percent of unsold share.
    </gds-p>

    <gds-p>
        Check the figures you have entered are correct. If they are, contact your Growth Manager to discuss this further.
    </gds-p>

    <div class="govuk-!-margin-top-9">
        <a class="govuk-button govuk-!-margin-right-4" data-module="govuk-button"
           href="@Url.OrganisationAction("Back", "HomeTypes", new { applicationId, homeTypeId, currentPage = HomeTypesWorkflowState.ProspectiveRentIneligible })">
            Go back and try again
        </a>
        <a class="govuk-button govuk-button--secondary" data-module="govuk-button"
           href="@Url.OrganisationAction("List", "HomeTypes", new { applicationId, homeTypeId })">
            Save and return to your account
        </a>
    </div>
</gds-div-grid-column-two-thirds>
