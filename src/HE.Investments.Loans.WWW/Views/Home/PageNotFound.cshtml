@using Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Page not found";
}

<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds-from-desktop">
        <a href="#" class="govuk-back-link" id="js-back-link">Back</a>

        <h1 class="govuk-heading-l">Page not found</h1>

        <p class="govuk-body">
            If you believe that you should be able to access this page, you can:
            <ul class="govuk-list govuk-list--bullet">
                <li>try again later</li>
                <li>
                    contact the Help to Build team:<br/>
                    <a href="mailto:helptobuildtechsupport@homesengland.gov.uk">helptobuildtechsupport@homesengland.gov.uk</a>
                </li>
            </ul>
        </p>
    </div>
</div>


<environment include="Development">
    <script src="~/javascripts/history.js" defer="defer"></script>
</environment>

<environment exclude="Development">
    <script src="~/javascripts/history.min.js" defer="defer"></script>
</environment>
