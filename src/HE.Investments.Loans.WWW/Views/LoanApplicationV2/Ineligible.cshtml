@using HE.Investments.Common.Gds
@using HE.Investments.Loans.BusinessLogic.LoanApplication
<gds-div-grid-column-two-thirds>
    <gds-back-link href="@Url.Action("Back","LoanApplicationV2", new { currentPage = LoanApplicationWorkflow.State.Ineligible})" text="Back" class="govuk-!-margin-bottom-8"></gds-back-link>
    <gds-h1 size="ControlSize.L">
        You are not eligible for a Levelling Up Home Building Fund Loan
    </gds-h1>
    <gds-p>Based on your answers, you are not eligible to apply for the levelling up home building fund.</gds-p>
    <gds-p>You were not eligible because you told us you require Homes England funding for something other than building new homes.</gds-p>
    <gds-p>You may be eligible to apply for a  <gds-link href="https://www.gov.uk/guidance/home-building-fund-infrastructure-loans">Home Building Fund Infrastructure Loan</gds-link> or another Homes England service.</gds-p>
</gds-div-grid-column-two-thirds>
