@using He.AspNetCore.Mvc.Gds.Components.TagHelpers.Buttons
@{
    if (!ViewData.TryGetValue("continueButtonText", out var continueButtonText))
    {
        continueButtonText = "Continue";
    }

}
<gds-button class="govuk-!-margin-bottom-8" data-module="govuk-button">
    @continueButtonText
</gds-button>
