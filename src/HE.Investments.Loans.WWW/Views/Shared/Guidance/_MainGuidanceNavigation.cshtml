@using HE.Investments.Common.Gds
@using He.AspNetCore.Mvc.Gds.Components.TagHelpers.Headings
@using He.AspNetCore.Mvc.Gds.Components.TagHelpers.Links
@using He.AspNetCore.Mvc.Gds.Components.TagHelpers.Lists
@using He.AspNetCore.Mvc.Gds.Components.TagHelpers.Paragraphs
@using He.AspNetCore.Mvc.Gds.Components.TagHelpers.Divs
<gds-h1 class="govuk-!-margin-top-9" size="ControlSize.Xl">
    Levelling Up Home Building Fund
</gds-h1>

<gds-p>If you're looking to build or refurbish homes for sale or rent, the Levelling Up Home Building Fund could provide you with a loan to help.</gds-p>

<gds-div-inset-text>
    Applies to England
</gds-div-inset-text>

<gds-p class="govuk-!-margin-bottom-3">
    Contents
    <ul class="govuk-list">
        <gds-li class="guidance-list_list-item">
            <gds-link href="@Url.Action("WhatTheHomeBuildingFundIs","Guidance")">What the Home Building Fund is</gds-link>
        </gds-li>
        <gds-li class="guidance-list_list-item">
            <gds-link href="@Url.Action("Eligibility","Guidance")">Eligibility</gds-link>
        </gds-li>
        <gds-li class="guidance-list_list-item">
            <gds-link href="@Url.Action("Apply","Guidance")">Apply</gds-link>
        </gds-li>
        <gds-li class="guidance-list_list-item">
            <gds-link href="@Url.Action("AfterApply","Guidance")">What happens after you apply</gds-link>
        </gds-li>
        <gds-li class="guidance-list_list-item">
            <gds-link href="@Url.Action("PrioritiseApplications","Guidance")">How we prioritise applications</gds-link>
        </gds-li>
    </ul>
</gds-p>
