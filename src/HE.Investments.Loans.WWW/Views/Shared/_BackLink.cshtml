@using He.AspNetCore.Mvc.Gds.Components.TagHelpers.Links
@{
    if (!ViewData.TryGetValue("backLink", out var backLink))
    {
        backLink = "javascript:window.history.back()";
    }
    if (!ViewData.TryGetValue("backLinkText", out var backLinkText))
    {
        backLinkText = "Back";
    }
}

<gds-back-link href=@backLink text=@backLinkText class="govuk-!-margin-bottom-8">
</gds-back-link>