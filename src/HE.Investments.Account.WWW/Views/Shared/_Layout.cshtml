@using HE.Investments.Account.WWW.Config
@using Microsoft.AspNetCore.Mvc.TagHelpers
@inject IOrganisationAppConfig OrganisationAppConfig

@{
    Layout = "_GovukTemplate";
    ViewData["ServiceName"] = OrganisationAppConfig.AppName;
}

@section AdditionalMeta
{
}

@{
    var cookieConsent = Context.Request.Cookies["CookieConsent"] == "Yes";
}

@section HeadCss
{
    <partial name="_LoadStylesheetPartial"/>
}

@section HeadScripts {

}

@section BeforeContent{
    <partial name="~/Partials/_Breadcrumbs.cshtml"/>
}

<gds-div-grid-row>
    @RenderBody()
</gds-div-grid-row>

@section BodyEnd
{
    <partial name="_LoadJavascriptPartial"/>
}
